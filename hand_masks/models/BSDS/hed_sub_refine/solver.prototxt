train_net: "models/BSDS/hed_sub_refine/train.prototxt"

# display
display: 10
average_loss: 100

# hyperparams are taken from Saining's paper
lr_policy: "step"
base_lr: 1e-6
gamma: 0.1
stepsize: 2500

iter_size: 10
momentum: 0.9
weight_decay: 0.0002

# save snapshot
snapshot: 0
snapshot_prefix: "hed_vgg16_sub_refine"