### Concat and multiscale weighted-fusion layer ###
layer {
  name: "concat"
  bottom: "upscore-dsn1"
  bottom: "upscore-dsn2"
  bottom: "upscore-dsn3"
  bottom: "upscore-dsn4"
  bottom: "upscore-dsn5"
  top: "concat-upscore"
  type: "Concat"
  concat_param {
    concat_dim: 1
  } 
}

layer {
  name: "new-score-weighting"
  type: "Convolution"
  bottom: "concat-upscore"
  top: "upscore-fuse"
  param {
    lr_mult: 0.01
    decay_mult: 1
  }
  param {
    lr_mult: 0.02
    decay_mult: 0
  }
  convolution_param {
    engine: CAFFE
    num_output: 1
    kernel_size: 1
    weight_filler { 
      type: "constant"
      value: 0.2
    }
  }
} 

# testing
layer { 
  type: "Sigmoid"
