## all the loss/"Crop" layers go here

## DSN conv 1 ###
layer { 
  name: "score-dsn1"
  type: "Convolution"
  bottom: "conv1_2"
  top: "score-dsn1-up"
  param {
    lr_mult: 0.01
    decay_mult: 1
  }
  param {
    lr_mult: 0.02
    decay_mult: 0
  }
  convolution_param {
    num_output: 1
    kernel_size: 1
    weight_filler {
      type: "constant"
      value: 0
    }
    engine: CAFFE
  }
}

layer { 
  type: "Crop"
  name: "crop1"
  bottom: "score-dsn1-up"
  bottom: "data"
  top: "upscore-dsn1" 
  crop_param {
    axis: 2
    offset: 34
    offset: 34
  }
  propagate_down: 1
  propagate_down: 0
}

