#### data layers end here #####
#### raw vgg layers start here #####

# block conv1 (fixed)
layer {
  name: "conv1_1"
  type: "Convolution"
  bottom: "data"
  top: "conv1_1"
  param {
    lr_mult: 0
    decay_mult: 0
