train_net: "models/GTEA/hed/train.prototxt"

# display
display: 10
average_loss: 100

# hyperparams are taken from Saining's paper
lr_policy: "step"
base_lr: 0.00000001
gamma: 0.1
stepsize: 16000

iter_size: 2
momentum: 0.9
weight_decay: 0.0002

# save snapshot
snapshot: 0
snapshot_prefix: "hand_vgg16"